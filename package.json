{
  "name": "sopt",
  "version": "0.0.1",
  "description": "A light-weighted yet powerful PT framework.",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-sopt",
    "stop": "egg-scripts stop --title=egg-server-sopt",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@hubcarl/json-typescript-mapper": "^2.0.0",
    "axios": "^0.21.1",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.0",
    "egg-view-vue-ssr": "^3.3.1",
    "egg-webpack": "^4.5.2",
    "egg-webpack-vue": "^2.0.2",
    "extend": "^3.0.2",
    "lodash": "^4.17.19",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "shortid": "^2.2.15",
    "vue": "^2.6.10",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "easywebpack-cli": "^4.6.0",
    "easywebpack-vue": "^4.5.0",
    "egg-bin": "^4.13.2",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "egg-scripts": "^2.11.0",
    "egg-ts-helper": "^1.25.6",
    "node-tool-utils": "^1.3.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "NJUPT-NYR",
  "license": "MIT"
}
